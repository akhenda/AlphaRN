{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "browser": true
  },
  "plugins": [
    "babel",
    "react",
    "react-native"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "arrow-body-style": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "func-names": 0,
    "global-require": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/label-has-for": 0,
    "no-trailing-spaces": 0,
    "no-console": 0,
    "no-confusing-arrow": 0,
    "no-unused-vars": 1,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "no-constant-condition": 0,
    "space-infix-ops": 0,
    "react/prefer-default-export": 0,
    "react/require-default-props": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-indent": 0,
    "import/named": 2
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
